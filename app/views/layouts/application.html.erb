<!DOCTYPE html>
<html>
  <head>
    <title>MeetNEat</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <link rel="shortcut icon" href="favicon.ico">
  </head>

  <body>
      <div class="container">
        <%= render 'layouts/header' %>
        <% if flash[:error] %>
          <%= flash[:error] %>
        <%end%>
        <%= yield %>
        <%= render 'layouts/footer' %>
      </div>
    </header>

  </body>
</html>
