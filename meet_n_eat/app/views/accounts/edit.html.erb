<h1> <%= @account.first_name%>'s Page </h1>
<p>Welcome to Meet N Eat</p>

<% if flash[:error] %>
<%= flash[:error] %>
<%end%>

<h1>Edit Your Account, Sucka:</h1>
<%= form_for (@account) do |f| %>
	<%= f.label :username %><br>
	<%= f.text_field :username %><br></br>
	<%= f.label :password %><br>
	<%= f.password_field :password%><br></br>
	<%= f.label :first_name %><br>
	<%= f.text_field :first_name %><br></br>
	<%= f.label :last_name%><br>
	<%= f.text_field :last_name %><br></br>
	<%= f.label :email %><br>
	<%= f.text_field :email %><br></br>

	<%= f.submit "Create Account"%>
<%end%><br>
<%=link_to "home", root_path %>
