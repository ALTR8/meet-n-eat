<video autoplay loop muted id="bgvid">
  <source src="" type="video/mp4">
</video>

<div class="center jumbotron">
<h1>Meet 'n' Eat!</h1>
<h2> Treat Yourself </h2>

<% if logged_in? %>
	<a href="/events" class="btn btn-danger" role="button" aria-pressed="true">Find Events</a>
	<a href="/events/new" class="btn btn-danger" role="button" aria-pressed="true">Create Event</a>
	<%= link_to "My Events", account_events_path(current_account), class: "btn btn-danger" %>
<% else %>
	<p>Log in to find meal buddies!<br><a href="/signin" class="btn btn-danger" role="button" aria-pressed="true">Log In</a></p>
	<p>Make A New Account!<br><a href="/accounts/new" class="btn btn-danger" role="button" aria-pressed="true">Sign Up</a></p>
	
<% end %>